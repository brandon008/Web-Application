{{body}}

<script>
	//https://stackoverflow.com/questions/6985507/one-time-page-refresh-after-first-page-load
	window.onload = function() {
    if(!window.location.hash) {
        window.location = window.location + '#loaded';
        window.location.reload();
    }
}
</script>
<ol>
	
{{#each userList}}
	<li>{{this}}</li>
{{/each}}
</ol>
